{
    "name": "letterboxdVPN",
    "description": "testing the extensions",
    "version": "1.0",
    "manifest_version": 3,

    "action":{
        "default_popup": "popup.html",
        "default_icon":{
            "16": "icon.png",
            "32": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons":{
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },

    "content_scripts": [
        {
          "matches": ["https://letterboxd.com/film/*"],
          "js": ["main.js"]
        }
    ]
}